{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Fluoridation by Water Company",
  "width": 650,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/eddent2009/Edden_FIT3179_DV2/refs/heads/main/data/Fluoridation%20by%20postcode%20-%20Sheet1.csv",
    "format": {"type": "csv"}
  },
  "mark": "arc",
  "encoding": {
    "theta": {
      "aggregate": "count",
      "field": "townsuburb",
      "type": "quantitative"
    },
    "color": {
      "field": "water_company",
      "type": "nominal",
      "scale": {"scheme": "category20"},
      "legend": {"title": "Water Company"}
    },
    "tooltip": [
      {"field": "water_company", "type": "nominal", "title": "Water Company"},
      {
        "field": "townsuburb",
        "type": "quantitative",
        "aggregate": "count",
        "title": "Suburb Count"
      }
    ]
  },
  "config": {}
}