{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Suburbs by Latitude and Longitude",
  "width": 800,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/eddent2009/Edden_FIT3179_DV2/refs/heads/main/data/Fluoridation%20by%20postcode%20-%20Sheet1.csv",
    "format": {"type": "csv"}
  },
  "mark": "point",
  "encoding": {
    "x": {
      "field": "lon",
      "type": "quantitative",
      "axis": {"title": "Longitude"},
      "scale": {"domain": [141, 150]}
    },
    "y": {
      "field": "lat",
      "type": "quantitative",
      "axis": {"title": "Latitude"},
      "scale": {"domain": [-39, -34]}
    },
    "color": {
      "field": "fluoride_level",
      "type": "nominal",
      "scale": {
        "domain": [
          "Fluoridated",
          "Natural fluoride",
          "Not fluoridated",
          "Partly fluoridated"
        ],
        "range": ["#377eb8", "#008000", "#FF0000", "#ff7f00"]
      },
      "legend": {"title": "Fluoride Level"}
    },
    "size": {"value": 50},
    "tooltip": [
      {"field": "townsuburb", "type": "nominal", "title": "Suburb"},
      {"field": "lat", "type": "quantitative", "title": "Latitude"},
      {"field": "lon", "type": "quantitative", "title": "Longitude"},
      {"field": "fluoride_level", "type": "nominal", "title": "Fluoride Level"}
    ]
  },
  "selection": {"grid": {"type": "interval", "bind": "scales"}},
  "config": {}
}